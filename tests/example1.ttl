@prefix : <http://example.org/ontology1#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Vehicle a owl:Class ;
    rdfs:label "Vehicle" ;
    rdfs:comment "A means of transportation" .

:Car a owl:Class ;
    rdfs:label "Car" ;
    rdfs:comment "A four-wheeled vehicle" ;
    rdfs:subClassOf :Vehicle .

:hasOwner a owl:ObjectProperty ;
    rdfs:label "has owner" ;
    rdfs:domain :Vehicle ;
    rdfs:range :Person .

:Person a owl:Class ;
    rdfs:label "Person" ;
    rdfs:comment "A human being" .

:hasColor a owl:DatatypeProperty ;
    rdfs:label "has color" ;
    rdfs:domain :Vehicle ;
    rdfs:range xsd:string .
