@prefix : <http://different.org/vocab#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Vehicle a owl:Class ;
    rdfs:label "Automobile" ;
    rdfs:comment "A motorized transport device" .

:Car a owl:Class ;
    rdfs:label "Passenger Car" ;
    rdfs:comment "A vehicle for personal transport" ;
    rdfs:subClassOf :Vehicle .

:hasOwner a owl:ObjectProperty ;
    rdfs:label "owned by" ;
    rdfs:domain :Car ;
    rdfs:range :Individual .

:Individual a owl:Class ;
    rdfs:label "Individual" ;
    rdfs:comment "A person or entity" .

:hasColor a owl:DatatypeProperty ;
    rdfs:label "color" ;
    rdfs:domain :Car ;
    rdfs:range xsd:string .
